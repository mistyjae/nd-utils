{
  "name": "nd-utils",
  "version": "0.1.0",
  "description": "nd common utils",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mistyjae/nd-utils.git"
  },
  "keywords": [
    "utils",
    "nd"
  ],
  "author": "",
  "scripts": {
    "build": "rimraf dist/* && node -r babel-register ./node_modules/webpack/bin/webpack.js --config ./webpack/production.config.js --progress --profile --colors",
    "build:ie8": "rimraf dist/* && node -r babel-register ./node_modules/webpack/bin/webpack.js --config ./webpack/ie8.config.js --progress --profile --colors",
    "karma": "node -r babel-register ./node_modules/karma/bin/karma start",
    "cover": "cross-env NODE_ENV=test nyc npm test",
    "lint": "eslint src --quiet",
    "test": "mocha --compilers js:babel-core/register -r babel-polyfill -r mock-local-storage  -r ignore-styles -r setup.js --recursive "
  },
  "license": "MIT",
  "files": [
    "lib",
    "package.json"
  ],
  "devDependencies": {
